{
  "name": "reply/web-authn",
  "description": "Shopware 6 plugin for passwordless authentication via WebAuthn API",
  "version": "v1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Portaltech Reply GmbH"
    }
  ],
  "type": "shopware-platform-plugin",
  "autoload": {
    "psr-4": {
      "Reply\\WebAuthn\\": "src/"
    }
  },
  "require": {
    "web-auth/webauthn-lib": "^2.1.7",
    "web-token/jwt-signature-algorithm-rsa": "^2.2.5",
    "web-token/jwt-key-mgmt": "^2.2.5"
  },
  "extra": {
    "shopware-plugin-class": "Reply\\WebAuthn\\ReplyWebAuthn",
    "label": {
      "de-DE": "WebAuthn Authentifizierung",
      "en-GB": "WebAuthn authentification"
    }
  },
  "archive": {
    "exclude": ["build*", "docker", ".gitlab-ci.yml", "*.phar"]
  }
}
